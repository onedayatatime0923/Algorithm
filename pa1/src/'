
#include <map>
#include "database.h"


void DataBase::compress(){
  map<int, int, IntCompare> wordMap;//(IntCompare(_parser));
  for (int i = 0;i < this->QuerySize(); ++i){
    map<int, int, IntCompare>::iterator iter = wordMap.find(i);
    if(iter != wordMap.end()){
      _wordlist[iter->second].add(i);
    }
    else{
      wordMap.insert(i);
      Word word(i);
    }

}

